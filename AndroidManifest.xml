            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dreamone.dev.MapActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".APITypeListActivity" >
        </activity>
        <activity android:name="com.dreamone.dev.ShareActivity" >
        </activity>
        <activity android:name=".BlogFunActivity" >
        </activity>
    </application>

</manifest>